{
  "name": "react-interactive-timeline",
  "version": "0.3.0",
  "description": "Customizable and interactive timeline component for React",
  "keywords": ["react", "timeline", "event", "time", "interactive", "story", "react-timeline"],
  "homepage": "https://mguihal.github.io/react-interactive-timeline",
  "author": "mguihal",
  "license": "MIT",
  "repository": "mguihal/react-interactive-timeline",
  "main": "dist/index",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "src/typings.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "rollup -c -w",
    "lint": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "prepare": "npm run build",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:deploy": "docz build && gh-pages -d .docz/dist"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-resize-detector": "^4.2.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^5.1.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/react-resize-detector": "^4.2.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.0",
    "docz": "^1.3.2",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^1.2.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.6",
    "rollup-plugin-postcss-modules": "^2.0.1",
    "rollup-plugin-typescript2": "^0.26.0",
    "rollup-plugin-url": "^3.0.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist",
    "src/typings.d.ts"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'src/**/*.{ts,tsx,css}'"
    }
  }
}
